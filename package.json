{
	"name": "rest-node-koa-typescript",
	"version": "1.0.0",
	"description": "API REST using NodeJS, KOA framework and TypesScript. Database used is MongoDB.",
	"main": "dist/app.js",
	"author": "Daniel Sousa",
	"repository": "https://github.com/TutoDS/typescript-api",
	"home": "https://github.com/TutoDS/typescript-api#readme",
	"license": "MIT",
	"engines": {
		"node": "12.x.x",
		"npm": "6.x.x",
		"yarn": "1.x.x"
	},
	"keywords": [
		"typescript",
		"nodejs",
		"koa",
		"swagger",
		"jest",
		"mongoose",
		"mongodb",
		"api"
	],
	"scripts": {
		"dev": "ts-node-dev -r tsconfig-paths/register --watch '**/*.ts,**/*.json,.env' --respawn --transpileOnly --no-notify --ignore-watch node_modules app.ts",
		"lint": "eslint . --ext ts,tsx",
		"lint:fix": "eslint . --ext ts,tsx --fix",
		"setup:dev": "ts-node -r tsconfig-paths/register ./src/scripts/setup/index.ts",
		"setup": "node ./dist/src/scripts/setup/index.ts"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/koa": "^2.11.3",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-router": "^7.4.1",
		"@types/koa-static": "^4.0.1",
		"@types/koa2-cors": "^2.0.1",
		"@types/mongoose": "^5.7.28",
		"@types/node": "^14.0.14",
		"@typescript-eslint/eslint-plugin": "^3.4.0",
		"@typescript-eslint/parser": "^3.4.0",
		"eslint": "^7.3.1",
		"prettier": "^2.0.5",
		"ts-node-dev": "^1.0.0-pre.49",
		"tsconfig-paths": "^3.9.0"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.3",
		"jsonwebtoken": "^8.5.1",
		"koa": "^2.13.0",
		"koa-bodyparser": "^4.3.0",
		"koa-router": "^9.0.1",
		"koa-static": "^5.0.0",
		"koa2-cors": "^2.0.6",
		"mongoose": "^5.9.20",
		"typescript": "^3.9.5"
	}
}