{
	"env": {
		"es2020": true,
		"node": true,
		"es6": true,
		"jest": true
	},
	"extends": [
		"airbnb-base"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {},
	"settings": {
		"import/resolver": {
			"typescript": {
				"directory": "./tsconfig.json"
			}
		}
	}
}